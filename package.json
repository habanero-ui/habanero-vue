{
  "name": "habanero-vue",
  "version": "0.0.0",
  "main": "./dist/habanero-vue.common.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name habanero-vue ./src/index.js",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist",
    "lint": "eslint --cache --fix src/**/*.{js,vue} && prettier --write src/**/*.{js,vue}",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "start": "start-storybook -p 6006"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "flatpickr": "^4.6.3",
    "lodash": "^4.17.15",
    "tippy.js": "6.2.6",
    "vue": "2.5.22"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/vue": "^5.3.14",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.2.0",
    "vue-template-compiler": "2.5.22"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.{css,js,vue}": "prettier --write"
  }
}
