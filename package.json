{
  "name": "habanero-vue",
  "version": "0.0.0",
  "main": "./dist/habanero-vue.common.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name habanero-vue ./src/index.js",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist",
    "lint": "eslint --cache --fix src/**/*.{js,vue} && prettier --write src/**/*.{js,vue}",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "start": "start-storybook -p 6006"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "flatpickr": "4.6.6",
    "fuzzysort": "1.1.4",
    "lodash": "4.17.20",
    "tippy.js": "6.2.7",
    "vue": "2.5.22"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "6.0.26",
    "@storybook/addon-backgrounds": "6.0.26",
    "@storybook/addon-controls": "6.0.26",
    "@storybook/addon-docs": "6.0.26",
    "@storybook/addon-links": "6.0.26",
    "@storybook/addon-viewport": "6.0.26",
    "@storybook/addons": "6.0.26",
    "@storybook/vue": "6.0.26",
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-eslint": "4.5.8",
    "@vue/cli-service": "4.5.8",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-preset-vue": "2.0.2",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "7.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "tailwindcss": "1.9.5",
    "vue-template-compiler": "2.5.22"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.{css,js,vue}": "prettier --write"
  }
}
